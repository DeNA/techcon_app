{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "category": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        },
        "icon": {
          "type": "string"
        }
      },
      "required": [
        "text",
        "icon"
      ]
    },
    "services": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "url"
          ]
        }
      ]
    },
    "stage": {
      "type": "string"
    },
    "start_at": {
      "type": "string"
    },
    "end_at": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "title": {
      "type": "string"
    },
    "text": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "stage",
    "start_at",
    "end_at",
    "tags",
    "title",
    "text"
  ]
}